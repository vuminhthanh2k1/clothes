{
  "name": "Clothes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "size": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "type": {
      "type": "string"
    },
    "photoURL": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "metaDescription": {
      "type": "string"
    },
    "inputPrice":{
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "categoryProduct": {
      "type": "belongsTo",
      "model": "CategoryProduct",
      "foreignKey": ""
    },
    "cart": {
      "type": "hasMany",
      "model": "Cart",
      "foreignKey": "",
      "through": "CartClothes"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "SUPERADMIN",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "ADMIN",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
